<h1>All Users</h1>
<table class = "table">
    <thead class= "thead">
    <tr>
        <td>Name</td>
        <td>Address</td>
        <td>phone_no</td>
        <td>gender</td>
        <td>email</td>
    
    </tr>
        
    </thead>
    
    <tbody>
        <% @user.each do |user|%>
        <tr>
            

            <td><%= user.name  %></td>
            <td><%= user.address %></td>
            <td><%= user.phone_no %></td>
            <td><%= user.gender %></td>
            <td><%= user.email%></td>
            
            <% end %>
        </tr>
    </tbody>
</table>
